public class MyClass {
    public static void main(String args[]) {
    	 
  int extension = 5;
        double extension1 = 0;
        Scanner s = new Scanner(System.in);

        for (int extensionpls = 0; extensionpls < extension; extensionpls++) {
            System.out.print("Inserta da number of extensions: ");
            extension1 += s.nextInt();
        }

        System.out.println("extensions here  " + (extension1 / extensionpls));
    }
}
